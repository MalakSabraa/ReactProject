{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "singularName": "employee",
    "pluralName": "employees",
    "displayName": "Employee"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "FirstName": {
      "type": "string",
      "required": true
    },
    "LastName": {
      "type": "string",
      "required": true
    },
    "Email": {
      "type": "email",
      "required": true
    },
    "Password": {
      "type": "password",
      "required": true
    },
    "Username": {
      "type": "string",
      "required": true
    },
    "Age": {
      "type": "integer",
      "required": true
    },
    "todos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::todo.todo",
      "mappedBy": "userId"
    }
  }
}
